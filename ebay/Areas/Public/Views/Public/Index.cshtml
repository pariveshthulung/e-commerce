<div class="product-section">
	<div class="container">
		<div class="row">
			@foreach (var product in Model.Data)
			{
				<div class="col-12 col-md-4 col-lg-3 mb-5 mb-md-0">

					<a class="product-item" asp-action="Detail" asp-route-id="@product.id">
						<div class="card">
							<img src="/Public/images/product/@product.Product_image"
								class="card-img-top rounded mx-auto d-block">
						</div>
						<div class="card-body">
							<div class="pl-1">
								<h3 class="product-title col-12 text-truncate">@product.Name</h3>
								<strong class="product-price">$@product.Price</strong>
								<span class="icon-cross">
									<img src="/Public/images/cross.svg" class="img-fluid">
								</span>
							</div>

						</div>
					</a>

				</div>
			}
		</div>
	</div>
</div>

